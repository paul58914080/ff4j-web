{
  "name": "ff4j-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "mock:server": "json-server --watch mock/db.json --port 3000",
    "start": "run-p mock:server \"ng serve\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint:ng": "ng lint",
    "lint:styles": "stylelint --config .lint/.stylelintrc.json src/app/**/*.scss",
    "lint:html": "htmlhint --config .lint/.htmlhintrc src/app/**/*.html",
    "lint": "run-p lint:ng lint:styles lint:html",
    "format:prettier": "lint-staged",
    "format:all": "run-s format:prettier lint",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@ng-bootstrap/ng-bootstrap": "16.0.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.4",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "^17.3.4",
    "@angular/compiler-cli": "^17.3.0",
    "@angular/localize": "^17.3.0",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "^8.57.0",
    "htmlhint": "^1.1.4",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.3",
    "json-server": "^1.0.0-alpha.23",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "stylelint": "^16.3.1",
    "stylelint-config-sass-guidelines": "^11.1.0",
    "stylelint-scss": "^6.2.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.2"
  }
}
